---
import { SITE } from "@data/constants";
import MainLayout from "@/layouts/MainLayout.astro";


const pageTitle: string = `Finish Up Package | ${SITE.title}`;
const metaDescription = "Complete your flight training with Blue Skies Above's Finish Up Package. Ideal for pilots with some flight hours, this program helps you achieve your certification efficiently.";
const ogTitle = "Finish Up Package | Blue Skies Above Flight School";
---

<MainLayout
  title={pageTitle}
  customDescription={metaDescription}
  customOgTitle={ogTitle}
  structuredData={{
    "@context": "https://schema.org",
    "@type": "WebPage",
    "@id": "",
    url: "",
    name: "About Blue Skies Above",
    description: metaDescription,
    isPartOf: {
      "@type": "WebSite",
      url: "",
      name: SITE.title,
      description: metaDescription,
    },
    inLanguage: "en-US",
  }}
>

</MainLayout>