---
import { Image } from "astro:assets";

const { src, alt } = Astro.props;

interface Props {
  src: string;
  alt: string;
  class?: string;
}
---

<Image
  class="inline-block h-8 w-8 rounded-full ring-2 ring-neutral-50 dark:ring-zinc-800"
  src={src}
  alt={alt}
  width={32}
  height={32}
  format="avif"
  loading="lazy"
  quality={80}
/>
